# AI Agents Environment Configuration
# Copy this to .env and fill in your actual API keys

# Required API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here

# Database Configuration (default values work for Docker Compose)
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Development Configuration (optional)
PYTHONPATH=./src

# Production Configuration (optional)
# BACKEND_URL=http://your-production-backend.com
# CORS_ORIGINS=["http://localhost:8501", "https://your-frontend.com"] 